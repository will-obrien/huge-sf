        <errorConditionFormula>ISPICKVAL(Intracompany_Sub_contract__c , &apos;Yes&apos;)  &amp;&amp;  ISBLANK( Parent_Contract__c )</errorConditionFormula>
        <errorDisplayField>Parent_Contract__c</errorDisplayField>
        <errorMessage>You must select a parent contract for Intracompany sub-contracts.</errorMessage>
    </validationRules>
