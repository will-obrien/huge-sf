    <fields>
        <fullName>is_exploratory_call__c</fullName>
        <description>Equals 1 if was scheduled as a first call (no matter whether it happened or was no show), 0 for other follow-up calls</description>
        <externalId>false</externalId>
        <formula>IF(OR(ISPICKVAL(ActivitySubtype__c, &quot;Call - exploratory&quot;), ISPICKVAL(ActivitySubtype__c, &quot;Call - exploratory no-show&quot;)) , 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Equals 1 if was scheduled as a first call (no matter whether it happened or was no show), 0 for other follow-up calls</inlineHelpText>
        <label>is exploratory call</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>no_shows__c</fullName>
        <description>Is equal to 1 if the call was a no-show, 0 otherwise</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL( ActivitySubtype__c, &quot;Call - exploratory no-show&quot;), 1, 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Is equal to 1 if the call was a no-show, 0 otherwise</inlineHelpText>
        <label>is no-shows</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <listViews>
