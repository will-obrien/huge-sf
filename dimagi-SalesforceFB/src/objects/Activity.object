    <fields>
        <fullName>Activity_Count__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
    OR(
        CONTAINS(
            LOWER(Subject),
            &quot;e-mail&quot;
        ),
        CONTAINS(
            LOWER(Subject),
            &quot;email&quot;
        )
    ),

    1, 

    NULL
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Email Count</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
