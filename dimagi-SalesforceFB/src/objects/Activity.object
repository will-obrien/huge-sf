    <fields>
        <fullName>Days_Overdue__c</fullName>
        <description>The last modified date minus the completed date. Will be zero for uncompleted tasks, zero for tasks completed before or on time, and a positive number for the number of days last modified beyond the due date.</description>
        <externalId>false</externalId>
        <formula>ABS(DATEVALUE(LastModifiedDate) - ActivityDate)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The last modified date minus the completed date. Will be blank for uncompleted tasks, zero for tasks completed before or on time, and a positive number for the number of days last modified beyond the due date.</inlineHelpText>
        <label>Days Overdue</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Days_Since_Due_Date__c</fullName>
        <externalId>false</externalId>
        <formula>Today() - ActivityDate</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Days Since Due Date</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
