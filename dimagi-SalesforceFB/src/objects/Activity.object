        <fullName>cirrusadv__Hour__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>FLOOR ( MOD( CreatedDate - ($System.OriginDateTime + 5/24),1) *24)</formula>
