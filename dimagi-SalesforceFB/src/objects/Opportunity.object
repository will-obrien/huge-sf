    <fields>
        <fullName>EV_Date_1_Minus_Expense_Income__c</fullName>
        <externalId>false</externalId>
        <formula>if(BLANKVALUE(Duration_months__c,0) = 0,
0,
if(CloseDate &gt; $Setup.Expected_Value__c.EV_Date_1__c,
0,
if((CloseDate + (30*Duration_months__c)) &lt; $Setup.Expected_Value__c.EV_Date_1__c,
  Expected_Value_Minus_Income__c ,
(   Expected_Value_Minus_Income__c  * ($Setup.Expected_Value__c.EV_Date_1__c -  CloseDate ) / (30 * Duration_months__c )))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>EV Date 1 Minus Expense Income</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
    <fields>
