    <fields>
        <fullName>Three_Month_EV__c</fullName>
        <externalId>false</externalId>
        <formula>IF( (MONTH(CloseDate ) + 12*YEAR(CloseDate)) &gt; (Month( today()) + 12*YEAR( today()) + 3) 
       || (BLANKVALUE(Duration_months__c,0) = 0)
       || (CloseDate &lt;  today()),
 0,
 if( (12*YEAR(today())+ MONTH(today()) + 3) &gt; (12*YEAR(CloseDate)+MONTH(CloseDate)+Duration_months__c),
  Expected_Value__c, 
   (Expected_Value__c  /  Duration_months__c) * (MONTH( today()) +  12*YEAR( today()) + 3 - MONTH(CloseDate) - 12*YEAR(CloseDate))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>3Month Cash (EV)</label>
        <precision>18</precision>
