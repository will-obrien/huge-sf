        <fullName>Opportunity_Validation__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   OR(ISBLANK( Product_Text__c ),Product_Text__c = &apos;TBD&apos;),
   &apos;Missing products&apos;,
   IF(
      ISBLANK(  Amount  ),
      &apos;Missing amount&apos;,
      IF(ISBLANK(  Duration_months__c  ),
        &apos;Missing duration&apos;,
        IF(ISBLANK(  Probability  ),  
          &apos;Missing probability&apos;,
          IF(
            AND(
                Amount = 0, 
                Probability = 0 
               ),
            &apos;Amount and Probability cannot be zero&apos;,
            IF(
              ISBLANK( Opportunity_Owning_Entity__c ),
              &apos;Opportunity Owner cannot be blank&apos;,
              IF(
                ISBLANK(  Implementing_Business_Unit__c ),
                &apos;Execution Owner cannot be blank&apos;,
                &apos;Check date - everything else is valid.&apos;
                )
              )
            )
          )
        )
     )
  )</formula>
