    <validationRules>
        <fullName>Change_Stage_Name</fullName>
        <active>false</active>
        <errorConditionFormula>ISCHANGED( StageName )  &amp;&amp;  $User.ProfileId  = &apos;00eb0000000gmdW&apos;</errorConditionFormula>
        <errorDisplayField>StageName</errorDisplayField>
        <errorMessage>You may not make changes to this field in Salesforce. Please edit the Fogbugz ticket.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Probability_not_zero_above_stage_4</fullName>
        <active>true</active>
        <description>This validation rule checks to see whether probability is zero for stages later than four, and throws an error if it is zero.</description>
        <errorConditionFormula>AND( 
    OR( 
       ISPICKVAL(StageName, &quot;Stage 4 - Proposal Development&quot;),
       ISPICKVAL(StageName, &quot;Stage 5 - Submitted&quot;),
       ISPICKVAL(StageName, &quot;Stage 6 - In Negotiations&quot;)
      ), 
    Probability = 0
    , ISBLANK(Fogbugz_Ticket_Number__c) 
   )</errorConditionFormula>
        <errorDisplayField>Probability</errorDisplayField>
        <errorMessage>Probability cannot be zero for Stage 4, Stage 5 or Stage 6 Opportunities.</errorMessage>
    </validationRules>
