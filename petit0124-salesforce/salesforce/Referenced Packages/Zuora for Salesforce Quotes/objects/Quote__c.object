    <validationRules>
        <fullName>ValidateCustomerAcceptanceDate</fullName>
        <active>true</active>
        <description>Validate whether Customer Acceptance Date is equal or after Service Activation Date</description>
        <errorConditionFormula>AND 
     ( NOT ( ISNULL (   Customer_Acceptance_Date__c   ) ),
       OR 
           ( 
             Customer_Acceptance_Date__c &lt;   Service_Activation_Date__c,
             Customer_Acceptance_Date__c &lt;    StartDate__c 
           )
     )</errorConditionFormula>
        <errorDisplayField>Customer_Acceptance_Date__c</errorDisplayField>
        <errorMessage>Customer Acceptance Date cannot be before Service Activation Date or Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ValidateServiceActivationDate</fullName>
        <active>true</active>
        <description>Validate whether Service Activation Date is equal or after Start Date</description>
        <errorConditionFormula>AND 
     ( NOT ( ISNULL (  Service_Activation_Date__c  ) ),
       Service_Activation_Date__c &lt;  StartDate__c 
     )</errorConditionFormula>
        <errorDisplayField>Service_Activation_Date__c</errorDisplayField>
        <errorMessage>Service Activation Date cannot be before Start Date</errorMessage>
    </validationRules>
