<apex:page cache="false" showHeader="false" contentType="text/css" controller="SSEPortalCSSController">

    /*CSS for the portal. Change at your own risk.
    Portal CSS additions should be put in SSEPortalCSSEdit*/
    
    /*Page positioning and size*/
        body{
            min-width:900px;
            <apex:outputPanel rendered="{!userTemplate.Image_Background__c == true}" layout="none">
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}') {!userTemplate.Back_Background_G_T__c};   
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}'), -moz-linear-gradient(top,  {!userTemplate.Back_Background_G_T__c} 0%, {!userTemplate.Back_Background_G_B__c} 100%); 
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}'), -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Background_G_T__c}), color-stop(100%,{!userTemplate.Back_Background_G_B__c}));
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}'), -webkit-linear-gradient(top,  {!userTemplate.Back_Background_G_T__c} 0%,{!userTemplate.Back_Background_G_B__c} 100%);
                background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Background_Image__c}'), linear-gradient(to bottom,  {!userTemplate.Back_Background_G_T__c} 0%,{!userTemplate.Back_Background_G_B__c} 100%);
                background-repeat:{!userTemplate.Image_Background_Repeat__c};
                background-position:{!userTemplate.Image_Background_Position__c};
            </apex:outputPanel>
            <apex:outputPanel rendered="{!userTemplate.Image_Background__c == false}" layout="none">
                background: {!userTemplate.Back_Background_G_T__c};   
                background: -moz-linear-gradient(top,  {!userTemplate.Back_Background_G_T__c} 0%, {!userTemplate.Back_Background_G_B__c} 100%); 
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Background_G_T__c}), color-stop(100%,{!userTemplate.Back_Background_G_B__c}));
                background: -webkit-linear-gradient(top,  {!userTemplate.Back_Background_G_T__c} 0%,{!userTemplate.Back_Background_G_B__c} 100%);
                background: linear-gradient(to bottom,  {!userTemplate.Back_Background_G_T__c} 0%,{!userTemplate.Back_Background_G_B__c} 100%);
            </apex:outputPanel>
        }
        
        /*Knowledge Header CSS 2015-01-29 Ben. Overrides Knowledge2.css*/
        /*
        body.hasMotif {
            margin: 0;
            padding: 0px !important;
        }
        */
        
        #portalwrapper{
            width:100%;
            min-height:100px;
            display:table;
            <apex:outputPanel rendered="{!userTemplate.Shadow_Body__c == true}" layout="none">
                box-shadow:0px 0px 10px #4d4d4d;
                -webkit-box-shadow:0px 0px 10px #4d4d4d;
                -moz-box-shadow:0px 0px 10px #4d4d4d;
            </apex:outputPanel>        
            margin:auto;
        }
        .bodyDiv{
            background:none !important;
            border:none;
            position:relative;
            top:-55px;
        }
        .outer td.oRight {
            padding: 0px;
            padding-left: 20px;
        }
        #portalheaderpadding{
            width:100%;
            height:36px;
        }
        #portalheader{
            width:100%;
            height:401px;
            <apex:outputPanel rendered="{!userTemplate.Shadow_Header__c == true}" layout="none">
                box-shadow:0px 0px 10px #4d4d4d;
                -webkit-box-shadow:0px 0px 10px #4d4d4d;
                -moz-box-shadow:0px 0px 10px #4d4d4d;
            </apex:outputPanel>
        }
        #portalheadertop{
            width:100%;
            height:36px;
            background: {!userTemplate.Back_Header_T_G_T__c};
            background: -moz-linear-gradient(top,  {!userTemplate.Back_Header_T_G_T__c} 0%, {!userTemplate.Back_Header_T_G_B__c} 15%); 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Header_T_G_T__c}), color-stop(15%,{!userTemplate.Back_Header_T_G_B__c}));
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Header_T_G_T__c} 0%,{!userTemplate.Back_Header_T_G_B__c} 15%);
            background: linear-gradient(to bottom,  {!userTemplate.Back_Header_T_G_T__c} 0%,{!userTemplate.Back_Header_T_G_B__c} 15%);
            position:fixed;
            top:0px;
            left:0px;
            z-index:101;
        }
        #portalheadermid{
            width:100%;
            height:100px;
            background: {!userTemplate.Back_Header_Mid_G_T__c};
            background: -moz-linear-gradient(top,  {!userTemplate.Back_Header_Mid_G_T__c} 0%, {!userTemplate.Back_Header_Mid_G_B__c} 100%); 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Header_Mid_G_T__c}), color-stop(100%,{!userTemplate.Back_Header_Mid_G_B__c}));
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Header_Mid_G_T__c} 0%,{!userTemplate.Back_Header_Mid_G_B__c} 100%);
            background: linear-gradient(to bottom,  {!userTemplate.Back_Header_Mid_G_T__c} 0%,{!userTemplate.Back_Header_Mid_G_B__c} 100%);
        }
        #portalheaderbot{
            width:100%;
            height:275px;
            background: {!userTemplate.Back_Header_B_G_T__c};
            background: -moz-linear-gradient(top,  {!userTemplate.Back_Header_B_G_T__c} 0%, {!userTemplate.Back_Header_B_G_B__c} 100%); 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Header_B_G_T__c}), color-stop(100%,{!userTemplate.Back_Header_B_G_B__c}));
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Header_B_G_T__c} 0%,{!userTemplate.Back_Header_B_G_B__c} 100%);
            background: linear-gradient(to bottom,  {!userTemplate.Back_Header_B_G_T__c} 0%,{!userTemplate.Back_Header_B_G_B__c} 100%);
        }
        #portalbody{
            width:900px;
            min-height:900px;
            margin:auto;
            margin-top:20px;
        }
        #portalfooter{
            width:100%;
            height:245px;
            border-top:5px solid {!userTemplate.Back_Header_B_G_T__c};
            background: {!userTemplate.Back_Footer_G_T__c};
            background-image:url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Footer__c}');
            background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Footer__c}'), -moz-linear-gradient(top,  {!userTemplate.Back_Footer_G_T__c}  0%, {!userTemplate.Back_Footer_G_B__c} 100%); 
            background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Footer__c}'), -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Footer_G_T__c} ), color-stop(100%,{!userTemplate.Back_Footer_G_B__c}));
            background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Footer__c}'), -webkit-linear-gradient(top,  {!userTemplate.Back_Footer_G_T__c}  0%,{!userTemplate.Back_Footer_G_B__c} 100%);
            background: url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Footer__c}'), linear-gradient(to bottom,  {!userTemplate.Back_Footer_G_T__c}  0%,{!userTemplate.Back_Footer_G_B__c} 100%);
            background-repeat:no-repeat;
            background-position:center top; 
            margin:auto;
            <apex:outputPanel rendered="{!userTemplate.Position_Footer__c == 'hide'}" layout="none">
                display:none;
            </apex:outputPanel>
        }
        .portalheadercenter{
            width: 900px;
            margin: auto;
            position:relative;
        }
        .portalfootercenter{
            width: 900px;
            height: 130px;
            margin: auto;
            padding-top: 15px;
        }
        #SSEPortalLogo{
            background-image:url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Logo__c}');
            background-repeat:no-repeat;
            background-position:left; 
            height:90px;
        }
        #homepageimage{
            margin-bottom:10px;
        }
        #homepageimage img{
            <apex:outputPanel rendered="{!userTemplate.Shadow_Page_Block__c == true}" layout="none">
                box-shadow:0px 0px 10px -2px #4D4D4D;
                -webkit-box-shadow:0px 0px 10px -2px #4D4D4D;
                -moz-box-shadow:0px 0px 0px 10px -2px #4D4D4D;
            </apex:outputPanel>
            border-radius:{!userTemplate.Radius_Page_Block__c};
            -webkit-border-radius:{!userTemplate.Radius_Page_Block__c};
            -moz-border-radius:{!userTemplate.Radius_Page_Block__c};
        }
        .oRight #homePageFrame{
            position:relative;
            top:-10px;
        }
        /*Classic portal look and feel*/
            <apex:outputPanel rendered="{!userTemplate.Position_Page__c == 'classic'}" layout="none">
                #portalwrapper{
                    width:940px;
                }
                #portalheader{
                    <apex:outputPanel rendered="{!userTemplate.Shadow_Body__c == true}" layout="none">
                        box-shadow:none;
                        -webkit-box-shadow:none;
                        -moz-box-shadow:none;
                    </apex:outputPanel>
                }
                #portalbody{
                    padding:20px;
                    background: {!userTemplate.Back_Body_G_T__c};
                    background: -moz-linear-gradient(top,  {!userTemplate.Back_Body_G_T__c} 0%, {!userTemplate.Back_Body_G_B__c} 100%); 
                    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Body_G_T__c}), color-stop(100%,{!userTemplate.Back_Body_G_B__c}));
                    background: -webkit-linear-gradient(top,  {!userTemplate.Back_Body_G_T__c} 0%,{!userTemplate.Back_Body_G_B__c} 100%);
                    background: linear-gradient(to bottom,  {!userTemplate.Back_Body_G_T__c} 0%,{!userTemplate.Back_Body_G_B__c} 100%);
                    margin-top:0px;
                }
                #portalfooter, #portalheader, .portalheadercenter{
                    width:940px;
                    margin:auto;
                    position: relative;
                }
            </apex:outputPanel>
    
    /*User profile menu*/
        #portalheaderiframe{
            border:0px;
            float:right;
            height:36px;
            width:240px;
            visibility:hidden;
            pointer-events:none;
        }
        #userborder{
            float:left;
            height:36px;
            width:1px;
            background:{!userTemplate.Back_Header_T_G_T__c};
            border-left:1px solid {!userTemplate.Back_Navigation_Menu__c};
            border-right:1px solid {!userTemplate.Back_Navigation_Menu__c};
        }
        #usersection{
            width:237px;
            height:35px;
            float:right;
            border-bottom:1px solid {!userTemplate.Back_Header_T_G_T__c};
            position:absolute;
            right:0px;
            z-index:-1;
            visibility:hidden;
        }
        #usersection:hover, #navsection:hover{
            cursor:pointer;
        }
        .userselected {
            background:{!userTemplate.Back_Navigation_Menu__c};
        }
        #usersection:hover #userarrow, #navsection:hover #userarrow{
            background: url('/img/chatterservice/btnArrow_sprite.png') 0 -327px no-repeat;
        }
        #username{
            color:{!userTemplate.Font_Colour_Navigation_Menu__c};
            font-size:{!userTemplate.Font_Size_Navigation_Menu__c};
            font-weight:{!userTemplate.Font_Weight_Navigation_Menu__c};
            text-transform:{!userTemplate.Font_Transform_Navigation_Menu__c};
            text-decoration:{!userTemplate.Font_Decoration_Navigation_Menu__c};
            line-height: 34px;
            margin-left: 56px;
        }
        #userarrow{
            width: 8px;
            height: 5px;
            background: url('/img/chatterservice/btnArrow_sprite.png') 0 -305px no-repeat;
            position: absolute;
            right: 12px;
            top: 16px;
            border: none;
            margin: 0;
        }
        #userimage{
            position: absolute;
            height: 20px;
            width: 20px;
            background: url('/resource/SSEPortalResources/images/SSEPortalUser.png');
            background-size: 100%;
            border: 2px solid white;
            top: 5px;
            left: 23px;
        }
        #usermenu{
            width:237px;
            position:absolute;
            right:0px;
            top:36px;
            border-radius: 0px 0px 4px 4px;
            -webkit-border-radius:0px 0px 4px 4px;
            -moz-border-radius:0px 0px 4px 4px;
            background:{!userTemplate.Back_Navigation_Menu__c};
            display:none;
        }
        .userlink{
            padding-left:20px;
            padding-top:10px;
            padding-bottom:10px;
            border-top:1px solid {!userTemplate.Back_Navigation_Menu_H__c};
            color:{!userTemplate.Font_Colour_Navigation_Menu__c};
            font-size:{!userTemplate.Font_Size_Navigation_Menu__c};
            font-weight:{!userTemplate.Font_Weight_Navigation_Menu__c};
            text-transform:{!userTemplate.Font_Transform_Navigation_Menu__c};
            text-decoration:{!userTemplate.Font_Decoration_Navigation_Menu__c};
        }
        .userlink:hover{
            background:{!userTemplate.Back_Navigation_Menu_H__c};
            cursor:pointer;
            color:{!userTemplate.Font_Colour_Navigation_Menu_H__c};
            font-size:{!userTemplate.Font_Size_Navigation_Menu_H__c};
            font-weight:{!userTemplate.Font_Weight_Navigation_Menu_H__c};
            text-transform:{!userTemplate.Font_Transform_Navigation_Menu_H__c};
            text-decoration:{!userTemplate.Font_Decoration_Navigation_Menu_H__c};
        }
        #link5{
            border-radius: 0px 0px 4px 4px;
            -webkit-border-radius:0px 0px 4px 4px;
            -moz-border-radius:0px 0px 4px 4px;
        }
 
    /*iPad navigation bar*/
        #navsection{
            width: 183px;
            height: 35px;
            position: absolute;
            top: 0px;
            left: 0px;
            color:{!userTemplate.Font_Colour_Navigation_Menu__c};
            font-size:{!userTemplate.Font_Size_Navigation_Menu__c};
            font-weight:{!userTemplate.Font_Weight_Navigation_Menu__c};
            text-transform:{!userTemplate.Font_Transform_Navigation_Menu__c};
            text-decoration:{!userTemplate.Font_Decoration_Navigation_Menu__c};
            border-bottom:1px solid {!userTemplate.Back_Header_T_G_T__c};
            line-height: 34px;
            padding-left: 20px;
            display:none;
        }
        #navsection #userborder{
            float:right;    
        }
    
    /*Image slideshow*/
        .slides_container{
            width:900px;
            height:275px;
            margin:auto;
            visibility:hidden;
        }
        .slides_container div{
            width:900px;
            height:275px;
        }
        <apex:outputPanel rendered="{!userTemplate.Position_Page__c == 'classic'}" layout="none">
            .slides_container, .slides_container div{
                width:940px;
            }
        </apex:outputPanel>
        #SSEPortalSlide1{
            background-image:url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Slide1__c}');
            background-repeat:no-repeat;
            background-position:center; 
        }
        #SSEPortalSlide2{
            background-image:url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Slide2__c}');
            background-repeat:no-repeat;
            background-position:center; 
        }
        #SSEPortalSlide3{
            background-image:url('{!LEFT($Site.CurrentSiteUrl, LEN($Site.CurrentSiteUrl)-1)}{!userTemplate.Image_Slide3__c}');
            background-repeat:no-repeat;
            background-position:center; 
        }
    
    /*Buttons*/
        .btn, .miniTab ul.miniTabList li{
            font-weight: normal;
            padding: 3px;
            padding-right: 5px;
            border: 0px;
            padding-left: 5px;
            border-radius: {!userTemplate.Radius_Button__c};
            -webkit-border-radius: {!userTemplate.Radius_Button__c};
            box-shadow: inset 0px 0px 6px -1px #333;
            -webkit-box-shadow: inset 0px 0px 6px -1px #333;
            <apex:outputPanel rendered="{!userTemplate.Image_Button__c == true}" layout="none">
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png') {!userTemplate.Back_Button_G_T__c};
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -moz-linear-gradient(top,  {!userTemplate.Back_Button_G_T__c}  0%, {!userTemplate.Back_Button_G_B__c} 100%); 
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Button_G_T__c} ), color-stop(100%,{!userTemplate.Back_Button_G_B__c}));
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -webkit-linear-gradient(top,  {!userTemplate.Back_Button_G_T__c}  0%,{!userTemplate.Back_Button_G_B__c} 100%);
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), linear-gradient(to bottom,  {!userTemplate.Back_Button_G_T__c}  0%,{!userTemplate.Back_Button_G_B__c} 100%);
            </apex:outputPanel>
            <apex:outputPanel rendered="{!userTemplate.Image_Button__c == false}" layout="none">
                background: {!userTemplate.Back_Button_G_T__c};
                background: -moz-linear-gradient(top,  {!userTemplate.Back_Button_G_T__c}  0%, {!userTemplate.Back_Button_G_B__c} 100%); 
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Button_G_T__c} ), color-stop(100%,{!userTemplate.Back_Button_G_B__c}));
                background: -webkit-linear-gradient(top,  {!userTemplate.Back_Button_G_T__c}  0%,{!userTemplate.Back_Button_G_B__c} 100%);
                background: linear-gradient(to bottom,  {!userTemplate.Back_Button_G_T__c}  0%,{!userTemplate.Back_Button_G_B__c} 100%);
            </apex:outputPanel>
        }
        .btn:hover, .miniTab ul.miniTabList li:hover{
            border:0px;
            <apex:outputPanel rendered="{!userTemplate.Image_Button_H__c == true}" layout="none">
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png') {!userTemplate.Back_Button_H_G_T__c};
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -moz-linear-gradient(top,  {!userTemplate.Back_Button_H_G_T__c}  0%, {!userTemplate.Back_Button_G_B__c} 100%); 
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Button_H_G_T__c} ), color-stop(100%,{!userTemplate.Back_Button_H_G_B__c}));
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), -webkit-linear-gradient(top,  {!userTemplate.Back_Button_H_G_T__c}  0%,{!userTemplate.Back_Button_H_G_B__c} 100%);
                background: url('/resource/SSEPortalResources/images/SSEPortalBarBackground.png'), linear-gradient(to bottom,  {!userTemplate.Back_Button_H_G_T__c}  0%,{!userTemplate.Back_Button_H_G_B__c} 100%);
            </apex:outputPanel>
            <apex:outputPanel rendered="{!userTemplate.Image_Button_H__c == false}" layout="none">
                background: {!userTemplate.Back_Button_H_G_T__c};
                background: -moz-linear-gradient(top,  {!userTemplate.Back_Button_H_G_T__c}  0%, {!userTemplate.Back_Button_G_B__c} 100%); 
                background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Button_H_G_T__c} ), color-stop(100%,{!userTemplate.Back_Button_H_G_B__c}));
                background: -webkit-linear-gradient(top,  {!userTemplate.Back_Button_H_G_T__c}  0%,{!userTemplate.Back_Button_H_G_B__c} 100%);
                background: linear-gradient(to bottom,  {!userTemplate.Back_Button_H_G_T__c}  0%,{!userTemplate.Back_Button_H_G_B__c} 100%);
            </apex:outputPanel>
        }
    
    /*Chatter Answers*/
        a.action{
            background: {!userTemplate.Back_Side_Button_G_T__c};
            background: -moz-linear-gradient(top, {!userTemplate.Back_Side_Button_G_T__c}  0%, {!userTemplate.Back_Side_Button_G_B__c} 100%); 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Side_Button_G_T__c} ), color-stop(100%,{!userTemplate.Back_Side_Button_G_B__c}));
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Side_Button_G_T__c}  0%,{!userTemplate.Back_Side_Button_G_B__c} 100%);
            background: linear-gradient(to bottom,  {!userTemplate.Back_Side_Button_G_T__c}  0%,{!userTemplate.Back_Side_Button_G_B__c} 100%);
            color:{!userTemplate.Font_Colour_Side_Button__c};
        }
        a.action:hover{
            background: {!userTemplate.Back_Side_Button_H_G_T__c};
            background: -moz-linear-gradient(top, {!userTemplate.Back_Side_Button_H_G_T__c}  0%, {!userTemplate.Back_Side_Button_H_G_B__c} 100%); 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Side_Button_H_G_T__c} ), color-stop(100%,{!userTemplate.Back_Side_Button_H_G_B__c}));
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Side_Button_H_G_T__c}  0%,{!userTemplate.Back_Side_Button_H_G_B__c} 100%);
            background: linear-gradient(to bottom,  {!userTemplate.Back_Side_Button_H_G_T__c}  0%,{!userTemplate.Back_Side_Button_H_G_B__c} 100%);  
            color:{!userTemplate.Font_Colour_Side_Button_H__c};
        }
        .leftContent li.selected a, .leftContent .feedcriterialinks li.selected a, #cs\:profile li.selected a, .togglePill .selected a, .togglePill .selected a:hover, .breadCrumbs .current span, .breadCrumbs .current a:hover span{
            background: {!userTemplate.Back_Page_Block_Header_G_B__c} !important;
            background: -moz-linear-gradient(top,  {!userTemplate.Back_Page_Block_Header_G_T__c} 0%, {!userTemplate.Back_Page_Block_Header_G_B__c} 100%) !important; 
            background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{!userTemplate.Back_Page_Block_Header_G_T__c}), color-stop(100%,{!userTemplate.Back_Page_Block_Header_G_B__c})) !important;
            background: -webkit-linear-gradient(top,  {!userTemplate.Back_Page_Block_Header_G_T__c} 0%,{!userTemplate.Back_Page_Block_Header_G_B__c} 100%) !important;
            background: linear-gradient(to bottom,  {!userTemplate.Back_Page_Block_Header_G_T__c} 0%,{!userTemplate.Back_Page_Block_Header_G_B__c} 100%) !important;  
            color:{!userTemplate.Font_Colour_Secondary_Title__c}
        }
        .togglePill .selected a, .togglePill .selected a:hover, .breadCrumbs .current span, .breadCrumbs .current a:hover span{
            border: 1px solid {!userTemplate.Back_Page_Block_Header_G_T__c} !important;
            color:{!userTemplate.Font_Colour_Secondary_Title__c}
        }
        .csfeedcontainer .feeditemcomment, .csfeedcontainer .feedcommentsshowmore, .csfeedcontainer .feeditemcommentplaceholder, .csfeedcontainer .feeditemaddcomment, .csfeedcontainer .header{
            background:{!userTemplate.Back_Page_Block_Table_Even__c};
        }
        .csfeedcontainer .showmorecommentscontainer a{
            background:{!userTemplate.Back_Page_Block_Table_Odd__c};
        }
        #cs\:searchAskInputContainer {
            border:1px solid {!userTemplate.Back_Side_Button_G_T__c};
        }
