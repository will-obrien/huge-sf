<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Allow_Blank_Opportunity_Names__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>If set, then Batch Data Entry will never name Opportunities, even if they are blank (not included on page).</inlineHelpText>
        <label>Allow Blank Opportunity Names</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Has_NPSP__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>This value is automatically created, and automatically configures the Batch Entry tool to work with the Nonprofit Starter Pack for donation entries.</inlineHelpText>
        <label>Has NPSP</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Opportunity_Naming__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>This setting automatically generates the record name for batch entered Opportunity records based on the value of the entered fields.</inlineHelpText>
        <label>Opportunity Naming</label>
        <type>Checkbox</type>
    </fields>
    <label>Batch Data Entry Config</label>
</CustomObject>
