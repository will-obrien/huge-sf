<apex:page controller="BINGOController" title="Dreamforce BINGO"
    showheader="true" sidebar="false" renderAs="{!chooserender}">

    <apex:stylesheet value="{! URLFOR($Resource.BingoCSS) }"/>

    <center><apex:form rendered="{!$CurrentPage.parameters.p == null}">
        <table border="0">
            <tr>
                <td width="200px">
                <center><apex:commandLink value="{!$Label.New_BINGO_Card}"
                    action="{!NewCard}" /></center>
                </td>
                <td width="200px">
                <center><apex:commandLink value="{!$Label.Show_as_PDF}"
                    action="{!showAsPDF}" target="_blank" /></center>
                </td>
            </tr>

        </table>
    </apex:form> <c:BingoCard >
        <apex:image id="CollaborationCloud"
            url="{!URLFOR($Resource.ChatterCloud)}" height="200" />
    </c:BingoCard></center>


</apex:page>