        <externalId>false</externalId>
        <formula>IF(
Probability = 1, TEXT(Probability*100)+&quot;%&quot; + &quot; &quot; + TEXT(StageName),
IF(
Probability &lt;&gt; 1, &quot;*&quot; + TEXT(Probability*100) + &quot;%&quot; + &quot; &quot; + TEXT(StageName), &quot;Error In Formula&quot;
))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
