    var userEmail = userRecord.getArray(&quot;records&quot;)[0].Email;
    var query = &quot;select id from contact where email=&apos;&quot; + userEmail + &quot;&apos; LIMIT 1&quot;;
    var istcif = &quot;select id from contact where email=&apos;istcif@pgi.com&apos; LIMIT 1&quot;;
    var istcifContact = sforce.connection.query(istcif);
    var userContact = sforce.connection.query(query);

    var istcifcontactID = istcifContact.getArray(&quot;records&quot;)[0].Id;
    var contactId = userContact.getArray(&quot;records&quot;)[0].Id;
    var singleRequest = new sforce.SingleEmailMessage();
    var templateId = emailTemplate.getArray(&quot;records&quot;)[0].Id;
    singleRequest.templateId = templateId;
    singleRequest.targetObjectId = contactId;
    singleRequest.whatId = &quot;{!Opportunity.Id}&quot;;
    singleRequest.saveAsActivity = false;
    var sendMailRes = sforce.connection.sendEmail([singleRequest]);
    //////////////////////////////////////
    var singleRequest2 = new sforce.SingleEmailMessage();
    singleRequest2.templateId = templateId;
    singleRequest2.targetObjectId = istcifcontactID;
    singleRequest2.whatId = &quot;{!Opportunity.Id}&quot;;
    singleRequest2.saveAsActivity = false;
    var sendMailRes = sforce.connection.sendEmail([singleRequest2]);
    //////////////////////////////////////
    alert(&quot;A new NCS has been generated&quot;);
    window.parent.location.href = window.parent.location.href;
}</url>
