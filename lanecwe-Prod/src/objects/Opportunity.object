    <fields>
        <fullName>z_30_Day_Buckets__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
      CloseDate - TODAY() &lt; 0, &quot;Close Date in Past&quot;,
IF(
      CloseDate - TODAY() &gt;= 0 &amp;&amp; CloseDate - TODAY() &lt; 31, &quot;0 - 30 Days&quot;,
IF(
      CloseDate - TODAY() &gt; 30 &amp;&amp; CloseDate - TODAY() &lt; 61, &quot;31 - 60 Days&quot;,
IF(
      CloseDate - TODAY() &gt; 60 &amp;&amp; CloseDate - TODAY() &lt; 91, &quot;61 - 90 Days&quot;,  
IF(
      CloseDate - TODAY() &gt;= 91, &quot;Close Date more than 90 Days in Future&quot;,&quot;Error in Formula Calculation, please notify salesforcecrmsupport@pgi.com with a screenshot&quot;
)))))</formula>
        <label>zz-30 Day Buckets</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
