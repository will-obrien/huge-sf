<apex:page standardController="dsfs__DocuSign_Envelope__c"
	Extensions="dsfs.DocuSignEnvelopeController" tabStyle="dsfs__DocuSign_Status__c"
	showHeader="false" sidebar="false" standardStylesheets="true"
	ID="TaggerDone">
	<apex:form >

		<script language="javascript"> 
       
            function getQuerystring(key, default_)
            {
              if (default_==null) default_=""; 
              key = key.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
              var regex = new RegExp("[\\?&]"+key+"=([^&#]*)");
              var qs = regex.exec(window.location.href);
              if(qs == null)
                return default_;
              else
                return qs[1];
            }

            function init()
            {
                var url = "/apex/docusign_SignNowPage?SourceID=" + getQuerystring("SourceID", "") + "&DSEID=" + getQuerystring("DSEID", "") + "&event=" + getQuerystring("event", "") + "&envelopeId=" + getQuerystring("envelopeId", "") + "&signNow=" + getQuerystring("signNow", "");

               if (window.parent == window)
                {
                    window.location = url;
                }
                else
                {
                    window.parent.location = url;
                }
            }
            
            if (window.attachEvent) {window.attachEvent('onload', init);}
            else if (window.addEventListener) {window.addEventListener('load', init, false);}
            else {document.addEventListener('load', init, false);} 
            
       </script>
	</apex:form>
</apex:page>