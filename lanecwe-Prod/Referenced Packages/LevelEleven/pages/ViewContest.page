<apex:page sidebar="false"
           controller="ePrize_Zemzu.ViewContestController"
           tabStyle="Contests__tab">

  <apex:composition template="ePrize_Zemzu__LeaderboardTemplate">

    <apex:define name="head">

    </apex:define>

    <apex:define name="contentHeader">
      <h1 class="lb-title">View Contest Details</h1>
      <h2 class="lb-subtitle">{!fullContest.contestEntity.ePrize_Zemzu__ContestName__c}</h2>
    </apex:define>

    <apex:define name="contentMain">
      <div class="zemzu">
        <apex:outputPanel rendered="{!IF(contestId != null, true, false)}">
          <apex:form >
            <!-- Special instructions to display if the contest is ready to be finalized. -->
            <apex:outputPanel rendered="{!fullContest.isReadyToBeFinalized}">
                  <!-- Instructions to contest administrator -->
                  <apex:outputPanel rendered="{!fullContest.isCurrentUserAdminAndCanFinalize || isCurrentUserSalesContestBuilderAdmin}">
                    <apex:pageBlock >
                      <p>This contest period has ended which means entries are no longer being tallied onto the leaderboard. This contest is currently waiting your review prior to finalizing. Following are the suggested steps prior to finalizing a contest:</p>
                      <ol>
                        <li>Click View Leaderboard to see the final contest results.</li>
                        <li>If anything doesn't look right for an individual user, click on their row in the leaderboard, and then click "View Entries".  This is in the right panel that includes individual user statistics.</li>
                        <li>If necessary you can Reject any entries which will modify the users contest data and may change their ranking.</li>
                        <li>If you feel anyone is missing entries, click on Ad Hoc Points to award a user more entries as necessary.</li>
                        <li>Once you are confident the leaderboard is accurate and final, come back to this Contest Details page and click "Finalize This Contest".</li>
                      </ol>
                    </apex:pageBlock>
                  </apex:outputPanel>
                  <!-- Instructions to non-administrators -->
                  <apex:outputPanel rendered="{!!fullContest.isCurrentUserAdmin && !isCurrentUserSalesContestBuilderAdmin}">
                      <apex:pageBlock >
                          <p>CONTEST WINNERS PENDING: This contest period has ended which means entries are no longer being tallied onto the leaderboard. The contest is currently being reviewed prior to announcing the final results and winners.</p>
                      </apex:pageBlock>
                  </apex:outputPanel>
            </apex:outputPanel>

            <apex:pageBlock title="Contest Review">
              <apex:pageMessages />
              <!-- The pageBlockButtons tag defines the buttons that appear at the top
                   and bottom of the pageBlock. Like a facet, it can appear anywhere in
                   a pageBlock, but always defines the button areas.
                   The Next button contained in this pageBlockButtons area
                   calls the step2 controller method, which returns a pageReference to
                   the next step of the wizard. -->
              <apex:pageBlockButtons >
                <apex:commandButton action="{!editContest}" value="Edit Contest" rendered="{!(fullContest.isCurrentUserAdmin || isCurrentUserSalesContestBuilderAdmin) && IF(fullContest.contestEntity.ePrize_Zemzu__EndDate__c > NOW(), true, false) && !usingNewBuilder}">
                   <apex:param name="contestId" value="{!contestId}"/>
                </apex:commandButton>
                <apex:outputPanel rendered="{!(fullContest.isCurrentUserAdmin || isCurrentUserSalesContestBuilderAdmin) && IF(fullContest.contestEntity.ePrize_Zemzu__EndDate__c > NOW(), true, false) && usingNewBuilder}">
                  <a href="/apex/CompeteBuilder#contest/{!contestId}/overview" class="btn">Edit </a>
                </apex:outputPanel>

                <apex:commandButton action="{!finalizeContest}" value="Finalize This Contest" rendered="{!(fullContest.isCurrentUserAdminAndCanFinalize || isCurrentUserSalesContestBuilderAdmin) && fullContest.isReadyToBeFinalized}">
                   <apex:param name="contestId" value="{!contestId}"/>
                </apex:commandButton>

                <apex:outputPanel rendered="{SalesContestBuilderUtil.usingLegacyWizards}">
                 <apex:commandButton action="{!cloneContest}" value="Clone Contest" rendered="{!(fullContest.isCurrentUserAdmin && fullcontest.IsCurrentUserCreator || isCurrentUserSalesContestBuilderAdmin) && (!fullContest.isParentContest || (!fullContest.isParentContest && !fullContest.isReadyToBeFinalized))}">
                     <apex:param name="contestId" value="{!contestId}"/>
                  </apex:commandButton>
                </apex:outputPanel>
                <apex:commandButton action="{!createChildContest}" value="Create Child Contest" rendered="{!((fullContest.isCurrentUserAdmin || isCurrentUserSalesContestBuilderAdmin) && !usingNewBuilder && fullContest.isParentContest) && (IF(fullContest.contestEntity.ePrize_Zemzu__EndDate__c > NOW(), true, false))}">
                   <apex:param name="contestId" value="{!contestId}"/>
                </apex:commandButton>

                <apex:commandButton action="{!deleteContest}" value="Delete Contest" onclick="return confirmDelete()" rendered="{!fullContest.isCurrentUserAdmin || isCurrentUserSalesContestBuilderAdmin}">
                   <apex:param name="contestId" value="{!contestId}"/>
                </apex:commandButton>

              </apex:pageBlockButtons>

              <apex:pageBlockSection title="Contest Information">
                <apex:pageBlockSectionItem rendered="{!fullContest.contestEntity.ParentContest__r.ePrize_Zemzu__ContestName__c != null}">
                   <apex:outputLabel value="Parent Contest" rendered="{!fullContest.contestEntity.ParentContest__r.ePrize_Zemzu__ContestName__c != null}" />
                   <apex:outputlink value="/apex/Leaderboard" rendered="{!fullContest.contestEntity.ParentContest__r.ePrize_Zemzu__ContestName__c != null}">
                {!fullContest.contestEntity.ParentContest__r.ePrize_Zemzu__ContestName__c}
                <apex:param name="contestId" value="{!fullContest.contestEntity.ParentContest__r.Id}"/>
                <apex:param name="sfdc.tabName" value="{!$CurrentPage.parameters.sfdc.tabName}"/>
               </apex:outputlink>
                   </apex:pageBlockSectionItem>
                   <apex:pageBlockSectionItem rendered="{!fullContest.contestEntity.ParentContest__r.ePrize_Zemzu__ContestName__c != null}" />
                <apex:pageBlockSectionItem >
                  <apex:outputLabel value="Point of Contact" for="contactPerson"/>
                  <apex:outputText id="contactPerson" value="{!fullContest.contestEntity.ContactPerson__r.FirstName} {!fullContest.contestEntity.ContactPerson__r.LastName} ({!fullContest.contestEntity.ContactPerson__r.Email})"/>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem />
                <apex:outputField id="contestName" value="{!fullContest.contestEntity.ePrize_Zemzu__ContestName__c}"/>
                <apex:pageBlockSectionItem />
            <apex:pageBlockSectionItem >
              <apex:outputLabel value="Contest Type"/>
              <apex:outputText >{!fullContest.contestEntity.ePrize_Zemzu__Goal__c}
              </apex:outputText>
            </apex:pageBlockSectionItem>
                <apex:outputField id="contestRankingStyle" value="{!fullContest.contestEntity.ePrize_Zemzu__RankingStyle__c}" />
                <apex:outputText value="" rendered="{!fullContest.isChanceToWin}" />
                <apex:outputField id="contestIWStatus" value="{!fullContest.contestEntity.ePrize_Zemzu__IWStatus__c}" rendered="{!fullContest.isChanceToWin}" />
                <apex:outputField id="contestStartDate" value="{!fullContest.contestEntity.ePrize_Zemzu__StartDate__c}"/>
                <apex:outputField id="contestEndDate" value="{!fullContest.contestEntity.ePrize_Zemzu__EndDate__c}"/>
                <apex:outputField id="contestFinalizedBy" value="{!fullContest.contestEntity.ePrize_Zemzu__FinalizedBy__c}" rendered="{!fullContest.contestEntity.ePrize_Zemzu__Status__c == 'Ended'}"/>
                <apex:outputField id="contestFinalizedDate" value="{!fullContest.contestEntity.ePrize_Zemzu__FinalizedOnDate__c}" rendered="{!fullContest.contestEntity.ePrize_Zemzu__Status__c == 'Ended'}"/>
              </apex:pageBlockSection>

              <!-- ###########################
                   ### - PARENT CONTESTS - ###
                   ########################### -->
              <apex:pageBlockSection rendered="{!fullContest.isParentContest}" title="Child Contests" columns="1">
                Child Contests
                <apex:pageBlockTable id="childContestsTable" value="{!childContests}" var="c" rowClasses="odd,even" >
                  <apex:column >
                <apex:facet name="header">NAME</apex:facet>
                  <apex:outputlink target="blank" value="/apex/ViewContest">
                    {!c.ePrize_Zemzu__ContestName__c}
                    <apex:param name="contestId" value="{!c.id}"/>
                  </apex:outputlink>
                </apex:column>
                <apex:column >
                <apex:facet name="header">START DATE</apex:facet>
                  <apex:outputField id="ChildcontestStartDate" value="{!c.ePrize_Zemzu__StartDate__c}"/>
                </apex:column>
                <apex:column >
                <apex:facet name="header">END DATE</apex:facet>
                  <apex:outputField id="ChildcontestEndDate" value="{!c.ePrize_Zemzu__EndDate__c}"/>
                </apex:column>
                <apex:column >
                <apex:facet name="header">STATUS</apex:facet>
                  <apex:outputText rendered="{!IF(c.ePrize_Zemzu__StartDate__c > NOW(), true, false)}" value="Not started" />
                  <apex:outputText rendered="{!IF(c.ePrize_Zemzu__isDraft__c, true, false)}" value="Draft" />
                  <apex:outputText rendered="{!IF(c.ePrize_Zemzu__StartDate__c <= NOW() && c.ePrize_Zemzu__EndDate__c > NOW() && !c.ePrize_Zemzu__isDraft__c, true, false)}" value="Running" />
                  <apex:outputText rendered="{!IF(c.ePrize_Zemzu__EndDate__c <= NOW() && c.ePrize_Zemzu__Status__c != null && c.ePrize_Zemzu__Status__c != '' && c.ePrize_Zemzu__Status__c != 'Ended', true, false)}" value="Winners Pending" />
                  <apex:outputText rendered="{!IF(c.ePrize_Zemzu__EndDate__c <= NOW() && (c.ePrize_Zemzu__Status__c == null || c.ePrize_Zemzu__Status__c == '' || c.ePrize_Zemzu__Status__c == 'Ended'), true, false)}" value="Ended" />
                </apex:column>
                <apex:column >
                  <apex:outputlink target="blank" value="/apex/Leaderboard">
                    Leaderboard
                    <apex:param name="contestId" value="{!c.id}"/>
                    <apex:param name="sfdc.tabName" value="{!$CurrentPage.parameters.sfdc.tabName}"/>
                  </apex:outputlink>
                </apex:column>
                </apex:pageBlockTable>
              </apex:pageBlockSection>

              <apex:pageBlockSection rendered="{!!fullContest.isParentContest}" title="Behaviors to Motivate" columns="1">
              <apex:dataList value="{!fullContest.pipelineBehaviors}" var="pb" id="pipelineBehaviorList" type="1" styleClass="pipelineBehavior-list">
              <!--  <apex:repeat value="{!fullContest.pipelineBehaviors}" var="pb"> -->
                  <apex:outputPanel >
                        <a href="#" onclick="jQuery('#{!pb.pipelineBehaviorEntity.Id}').toggle(); return false;">
                            <b><apex:outputText value="{!pb.pipelineBehaviorEntity.Name}" />
                            <apex:outputText value=" - {!Round(pb.pipelineBehaviorEntity.ePrize_Zemzu__Quantity__c,0)} Point" rendered="{!IF(fullContest.isEntryRanked, true, false)}" />
                            <apex:outputText value="s" rendered="{!IF(fullContest.isEntryRanked && pb.pipelineBehaviorEntity.ePrize_Zemzu__Quantity__c > 1, true, false)}" /></b>
                           </a>
                           <br/>
                        <apex:outputText styleClass="font-gray font-smaller font-italic" value="{!pb.pipelineBehaviorEntity.ePrize_Zemzu__Description__c}" />

                        <div id="{!pb.pipelineBehaviorEntity.Id}" style="">

                        <!-- #########################
                             ### - CASE CONTEST -  ###
                             ######################### -->
                        <apex:outputPanel rendered="{!fullContest.isCaseContest}">
                          <apex:pageBlockSection columns="1">
                            <apex:pageBlockSection columns="1">
                            <apex:outputField value="{!pb.pipelineBehaviorEntity.ePrize_Zemzu__CasesCreated__c}" rendered="{!pb.pipelineBehaviorEntity.ePrize_Zemzu__CasesCreated__c}"/>
                            <apex:pageBlockTable id="caseCreatedCustomFiltersTable" value="{!pb.caseCreatedCustomFieldValueFilters}" rendered="{!IF(pb.caseCreatedCustomFieldValueFilters.size > 0, true, false)}" var="t" rowClasses="odd,even" >
                                <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                                <apex:column >
                  <apex:facet name="header">
                  Field
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Comparator
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__Comparator__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Value
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__CompareToValue__c}" />
                </apex:column>

                              </apex:pageBlockTable>
                              <apex:pageBlockTable id="caseCreatedRelatedAccountCustomFieldValueFiltersTable" value="{!pb.caseCreatedRelatedAccountCustomFieldValueFilters}" rendered="{!IF(pb.caseCreatedRelatedAccountCustomFieldValueFilters.size > 0, true, false)}" var="t" rowClasses="odd,even" >
                                <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                                <apex:column >
                  <apex:facet name="header">
                  Field
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Comparator
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__Comparator__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Value
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__CompareToValue__c}" />
                </apex:column>

                              </apex:pageBlockTable>
                              <apex:pageBlockTable id="createdCustomFiltersTable" value="{!pb.otherCustomFieldValueFilters}" rendered="{!IF(pb.otherCustomFieldValueFilters.size > 0, true, false)}" var="t" rowClasses="odd,even" >
                                <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                                <apex:column >
                  <apex:facet name="header">
                  Field
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Comparator
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__Comparator__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Value
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__CompareToValue__c}" />
                </apex:column>

                              </apex:pageBlockTable>
                            <apex:outputField value="{!pb.pipelineBehaviorEntity.ePrize_Zemzu__CasesModified__c}" rendered="{!pb.pipelineBehaviorEntity.ePrize_Zemzu__CasesModified__c}"/>
                            <apex:pageBlockTable id="caseModifiedCustomFiltersTable" value="{!pb.caseModifiedCustomFieldValueFilters}" rendered="{!IF(pb.caseModifiedCustomFieldValueFilters.size > 0, true, false)}" var="t" rowClasses="odd,even" >
                                <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                                <apex:column >
                  <apex:facet name="header">
                  Field
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Comparator
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__Comparator__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Value
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__CompareToValue__c}" />
                </apex:column>

                              </apex:pageBlockTable>
                              <apex:inputCheckbox disabled="true" selected="true" rendered="{!IF(pb.caseModifiedRelatedAccountCustomFieldValueFilters.size > 0, true, false)}"/>
                              <apex:pageBlockTable id="caseModifiedRelatedAccountCustomFieldValueFiltersTable" value="{!pb.caseModifiedRelatedAccountCustomFieldValueFilters}" rendered="{!IF(pb.caseModifiedRelatedAccountCustomFieldValueFilters.size > 0, true, false)}" var="t" rowClasses="odd,even" >
                                <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                                <apex:column >
                  <apex:facet name="header">
                  Field
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Comparator
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__Comparator__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Value
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__CompareToValue__c}" />
                </apex:column>

                              </apex:pageBlockTable>
                              <apex:pageBlockTable id="modifiedCustomFiltersTable" value="{!pb.otherCustomFieldValueFilters}" rendered="{!IF(pb.otherCustomFieldValueFilters.size > 0, true, false)}" var="t" rowClasses="odd,even" >
                                <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                                <apex:column >
                  <apex:facet name="header">
                  Field
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__FieldName__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Comparator
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__Comparator__c}" />
                </apex:column>

                <apex:column >
                  <apex:facet name="header">
                    Value
                  </apex:facet>
                  <apex:outputText value="{!t.ePrize_Zemzu__CompareToValue__c}" />
                </apex:column>

                              </apex:pageBlockTable>
                              </apex:pageBlockSection>


                          </apex:pageBlockSection>
                        </apex:outputPanel>

                             <!-- END OF CASE CONTEST -->

                        <!-- #########################
                             ### - CLOSING SALES - ###
                             ######################### -->
                        <apex:outputPanel rendered="{!fullContest.isClosingSales}">
                          <apex:pageBlockSection columns="1">
                            <apex:pageBlockSectionItem >
                              <apex:outputLabel value="Sales Stage" for="opportunitiesModifiedClosingSalesStageValue"/>
                              <apex:outputText id="opportunitiesModifiedClosingSalesStageValue" value="{!pb.salesStage.MasterLabel}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem />
                            <apex:outputField id="opportunitiesModifiedClosingMinAmount" value="{!pb.opportunityFilterModified.ePrize_Zemzu__MinAmount__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__MinAmount__c}"/>
                            <apex:outputField id="opportunitiesModifiedClosingMinAmountValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__MinAmountValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__MinAmount__c}"/>
                            <apex:outputField id="opportunitiesModifiedClosingMaxAmount" value="{!pb.opportunityFilterModified.ePrize_Zemzu__MaxAmount__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__MaxAmount__c}"/>
                            <apex:outputField id="opportunitiesModifiedClosingMaxAmountValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__MaxAmountValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__MaxAmount__c}"/>
                            <apex:outputField id="opportunitiesModifiedClosingCloseDateStart2" value="{!pb.opportunityFilterModified.ePrize_Zemzu__CloseDateStart2__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__CloseDateStart2__c}"/>
                            <apex:outputField id="opportunitiesModifiedClosingCloseDateStart2Value" value="{!pb.opportunityFilterModified.ePrize_Zemzu__CloseDateStart2Value__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__CloseDateStart2__c}"/>
                            <apex:outputField id="opportunitiesModifiedClosingCloseDateEnd2" value="{!pb.opportunityFilterModified.ePrize_Zemzu__CloseDateEnd2__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__CloseDateEnd2__c}"/>
                            <apex:outputField id="opportunitiesModifiedClosingCloseDateEnd2Value" value="{!pb.opportunityFilterModified.ePrize_Zemzu__CloseDateEnd2Value__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__CloseDateEnd2__c}"/>
                        <apex:outputField id="opportunitiesModifiedClosingCampaign" value="{!pb.opportunityFilterModified.ePrize_Zemzu__Campaign__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__Campaign__c}"/>
                        <apex:outputField id="opportunitiesModifiedClosingCampaignValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__CampaignValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__Campaign__c}"/>

                            <apex:outputField id="opportunitiesModifiedAccount" value="{!pb.opportunityFilterModified.ePrize_Zemzu__Account__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__Account__c}"/>
                            <apex:pageBlockSection columns="2" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__Account__c}">
                              <apex:outputField id="opportunitiesModifiedClosingAccountName" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountName__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountName__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountNameValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountNameValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountName__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountRevenueMin" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRevenueMin__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRevenueMin__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountRevenueMinValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRevenueMinValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRevenueMin__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountRevenueMax" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRevenueMax__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRevenueMax__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountRevenueMaxValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRevenueMaxValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRevenueMax__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountIndustry" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountIndustry__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountIndustry__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountIndustryValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountIndustryValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountIndustry__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountRating" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRating__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRating__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountRatingValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRatingValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountRating__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountType" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountType__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountType__c}"/>
                              <apex:outputField id="opportunitiesModifiedClosingAccountTypeValue" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountTypeValue__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountType__c}"/>
                              <apex:outputField id="opportunitiesModifiedAccountCustomFieldValueFilter" value="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountCustomFieldValueFilter__c}" rendered="{!pb.opportunityFilterModified.ePrize_Zemzu__AccountCustomFieldValueFilter__c}"/>
                              <apex:pageBlockSectionItem >
                                  <apex:outputPanel layout="block" style="padding-left: 90px;">
