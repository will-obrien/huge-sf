                    }
                }
            }else{
                if(ps_allSelectedIDs_revoke == '' && user_allSelectedIDs_revoke == ''){
                    ps_currentSelectedUserLicenseRevoke = null;
                    displayPScurrentSelectedLicenseRevoke();
                }
            }
            // ----

            if(!allChecked){
