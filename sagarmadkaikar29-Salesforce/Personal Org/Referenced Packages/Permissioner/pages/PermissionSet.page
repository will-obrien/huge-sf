            var ps_id = getIDofPScheckboxRevoke(clickedObj);
            if(clickedObj.checked){
                if((ps_count == 0)
                    || (u_count == 0 && ps_count < assign_maxASSIGNMENTS)
                    || (u_count > 0 && (ps_count + 1) * u_count <= assign_maxASSIGNMENTS)
                ){
