                ){
                    user_addPSidRevoke(user_id);
                }else{
                    clickedObj.checked = false;
                    allChecked = false;
                    assignmenst_limit_reach = true;
                }
            }else{
                user_removePSidRevoke(user_id)
            }
            // ----

