    <validationRules>
        <fullName>Admin_Only_changes_to_Promotion_Campaign</fullName>
        <active>false</active>
        <description>Only allow System Admins to make changes to Promotion Campaigns</description>
        <errorConditionFormula>AND(
RecordTypeId = &apos;012i0000000xbyx&apos;,
$User.ProfileId != &apos;00ei0000000xu3a&apos;,
$User.ProfileId != &apos;00ei0000000U7A9&apos;
)</errorConditionFormula>
        <errorMessage>Currently, only Admins can make changes to Promotion Campaigns.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Advocacy_Form_State_Restriction</fullName>
        <active>true</active>
        <description>Prevents a scenario where an action is state-restricted but does not ask for State, rendering the action un-takeable.</description>
        <errorConditionFormula>RecordTypeId = &apos;012i0000000xuva&apos; 
&amp;&amp; NOT(INCLUDES( kwac__Permitted_States__c , &apos;All States&apos;)) 
&amp;&amp; NOT(INCLUDES( kwac__Fields_to_Populate__c, &apos;All&apos;)) 
&amp;&amp; NOT(INCLUDES( kwac__Fields_to_Populate__c, &apos;State&apos;)) 
&amp;&amp; $User.Alias &lt;&gt; &apos;guest&apos; 
&amp;&amp; $User.Alias &lt;&gt; &apos;ciron&apos;
&amp;&amp; $User.Alias &lt;&gt; &apos;bsd&apos;
&amp;&amp; $Profile.Id &lt;&gt; &apos;00ei0000000xu3a&apos;</errorConditionFormula>
        <errorDisplayField>kwac__Fields_to_Populate__c</errorDisplayField>
        <errorMessage>If your action is state-restricted, you must display the State field or display All fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Club_Entity_Must_Match_Tactic_Owner</fullName>
        <active>true</active>
        <errorConditionFormula>$Profile.Id  &lt;&gt; &apos;00ei0000000xu3a&apos;
&amp;&amp; $User.Alias &lt;&gt; &apos;ciron&apos;
&amp;&amp; $User.Alias &lt;&gt; &apos;guest&apos;
&amp;&amp; NOT(ISBLANK( Club_Entity__c ))
&amp;&amp; 
(
(ISPICKVAL(Tactic_Owner_Global__c, &apos;Inspiring Connections Outdoors&apos;) &amp;&amp; NOT(ISPICKVAL( Club_Entity__r.Entity_Category__c, &apos;Outings&apos;)))
|| (ISPICKVAL(Tactic_Owner_Global__c, &apos;Activity Sections&apos;) &amp;&amp; NOT(ISPICKVAL( Club_Entity__r.Entity_Type__c, &apos;Activity Sections&apos;)))
|| (ISPICKVAL(Tactic_Owner_Global__c, &apos;Chapter&apos;) &amp;&amp; NOT(ISPICKVAL( Club_Entity__r.Entity_Type__c, &apos;Chapter&apos;)) &amp;&amp; NOT(ISPICKVAL( Club_Entity__r.Entity_Type__c, &apos;Special Entity&apos;)))
|| (ISPICKVAL(Tactic_Owner_Global__c, &apos;Group&apos;) &amp;&amp; NOT(ISPICKVAL( Club_Entity__r.Entity_Type__c, &apos;Group&apos;)))
|| (ISPICKVAL(Tactic_Owner_Global__c, &apos;Sierra Student Coalition&apos;) &amp;&amp; NOT(ISPICKVAL( Club_Entity__r.Entity_Category__c, &apos;SSC&apos;)))
)</errorConditionFormula>
        <errorDisplayField>Club_Entity__c</errorDisplayField>
        <errorMessage>Club Entity/Team must be of same type as the Tactic Owner chosen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Default_Subject_Line_Required</fullName>
        <active>true</active>
        <errorConditionFormula>$User.Alias &lt;&gt; &apos;guest&apos; 
&amp;&amp; 
$User.Alias &lt;&gt; &apos;ciron&apos; 
&amp;&amp; 
$User.Alias &lt;&gt; &apos;bsd&apos; 
&amp;&amp; 
RecordType.Name = &apos;Advocacy Form&apos; 
&amp;&amp; 
ISPICKVAL(kwac__Action_Type__c, &apos;Contact Officials - All info required&apos;) 
&amp;&amp; 
ISBLANK(kwac__Default_Subject_Line__c)</errorConditionFormula>
        <errorDisplayField>kwac__Default_Subject_Line__c</errorDisplayField>
        <errorMessage>If your action delivers messages to recipients, a Default Subject Line is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Donation_Form_in_Redirect</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(kwac__Thank_You_Page__c)) &amp;&amp; 
CONTAINS(kwac__Thank_You_Page__c, &apos;sierra.secure.force.com/donate&apos;) &amp;&amp; 
((NOT(CONTAINS(kwac__Thank_You_Page__c, &apos;?id&apos;)) &amp;&amp; NOT(CONTAINS(kwac__Thank_You_Page__c, &apos;&amp;id&apos;))) || NOT(CONTAINS(kwac__Thank_You_Page__c, &apos;formcampaignid&apos;)))</errorConditionFormula>
        <errorDisplayField>kwac__Thank_You_Page__c</errorDisplayField>
        <errorMessage>Donation form redirects must contain both id and formcampaign parameters.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>KnowWho_Default_Message_Length</fullName>
