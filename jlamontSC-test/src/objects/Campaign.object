        <active>true</active>
        <description>This rule states that Start Date/Time must be before the End Date/Time.</description>
        <errorConditionFormula>AND( $Setup.rC_Event__Campaign_Validation__c.rC_Event__Start_Date_Time_Before_End_Date_Time__c , OR(RecordType.Name = &apos;Event Session&apos;,RecordType.Name = &apos;Event&apos;,RecordType.Name = &apos;Event Template&apos;), NOT( ISBLANK( rC_Event__Start_Date_Time__c ) ),  NOT( ISBLANK( rC_Event__End_Date_Time__c ) ),   rC_Event__Start_Date_Time__c &gt; rC_Event__End_Date_Time__c  )</errorConditionFormula>
        <errorMessage>Start Date/Time must be before the End Date/Time</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rC_Event__Waitlisted_Limit_Invalid</fullName>
        <active>true</active>
        <description>This rule states that if the Registered limit is 0, Waitlisted limit should not be allowed to enter.</description>
        <errorConditionFormula>AND($Setup.rC_Event__Campaign_Validation__c.rC_Event__Waitlisted_Limit_Invalid__c, RecordType.Name = &apos;Event Session&apos;, rC_Event__Registered_Limit__c  =  0, NOT(ISNULL(rC_Event__Waitlisted_Limit__c)))</errorConditionFormula>
        <errorMessage>If the registered limit is 0, there should be no value allowed in the Waitlisted Limit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rC_Giving__Effort_Invalid</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(TEXT(rC_Giving__Effort__c))), OR(VALUE(TEXT(rC_Giving__Effort__c)) &lt; 0, VALUE(TEXT(rC_Giving__Effort__c)) &gt; 99))</errorConditionFormula>
        <errorDisplayField>rC_Giving__Effort__c</errorDisplayField>
        <errorMessage>Invalid value: Effort</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>rC_Giving__Giving_Type_Invalid</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(CASE(rC_Giving__Giving_Type__c, &apos;&apos;, 1, &apos;New&apos;, 1, &apos;Additional&apos;, 1, &apos;Upgrade&apos;, 1, &apos;Renewal&apos;, 1, &apos;Rejoin&apos;, 1, NULL)))</errorConditionFormula>
        <errorDisplayField>rC_Giving__Giving_Type__c</errorDisplayField>
        <errorMessage>Invalid picklist value: Giving Type</errorMessage>
    </validationRules>
