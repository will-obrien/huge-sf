        <fullName>Likes_Link__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(Type__c , &apos;Private Message&apos;),
IMAGE($Setup.Twitter_Public_Configuration__c.Like_Me_Icon_Url__c,&apos;like&apos;,16,16) + &apos; &apos; +
HYPERLINK(
		&apos;javascript:alert(&quot;&apos; + $Label.FACEBOOK_PUBLIC_ONLY + &apos;&quot;)&apos;,		
		$Label.Like
	),

IF( ISPICKVAL(Type__c , &apos;Outbound Private Message&apos;),
IMAGE($Setup.Twitter_Public_Configuration__c.Like_Me_Icon_Url__c,&apos;like&apos;,16,16) + &apos; &apos; +
HYPERLINK(
		&apos;javascript:alert(&quot;&apos; + $Label.FACEBOOK_PUBLIC_ONLY + &apos;&quot;)&apos;,		
		$Label.Like
),
IF(User_Likes__c,
	HYPERLINK(
		IF(ISPICKVAL(Origin__c, &apos;Facebook&apos;),
			&apos;javascript:if(window.location.href.indexOf(&quot;emptyHtmlDoc.html&quot;) &gt;= 0) {parent.location= &quot;/apex/sf4twitter__FcbkFeatures?op=unlike&amp;records=&apos; + Id + &apos;&amp;retURL=&quot; + encodeURIComponent(parent.location);} else {window.location= &quot;/apex/sf4twitter__FcbkFeatures?op=unlike&amp;records=&apos; + Id + &apos;&amp;retURL=&quot; + encodeURIComponent(window.location);} &apos;,
			&apos;javascript:alert(&quot;&apos; + $Label.Like_Error_Message + &apos;&quot;)&apos;
		),
		$Label.Unlike, &apos;_self&apos;
	) + &apos; &apos; +
	CASE(Likes__c,
		1, $Label.You_Like_This,
		2, $Label.You_And_1_Like_This,
		SUBSTITUTE($Label.You_And_Others_Like_This, &apos;{0}&apos;, TEXT(Likes__c - 1))
	)
,
	IMAGE($Setup.Twitter_Public_Configuration__c.Like_Me_Icon_Url__c,&apos;like&apos;,16,16) + &apos; &apos; +
	HYPERLINK(
		IF(ISPICKVAL(Origin__c, &apos;Facebook&apos;),
			&apos;javascript:if(window.location.href.indexOf(&quot;emptyHtmlDoc.html&quot;) &gt;= 0) {parent.location= &quot;/apex/sf4twitter__FcbkFeatures?op=like&amp;records=&apos; + Id + &apos;&amp;retURL=&quot; + encodeURIComponent(parent.location);} else {window.location= &quot;/apex/sf4twitter__FcbkFeatures?op=like&amp;records=&apos; + Id + &apos;&amp;retURL=&quot; + encodeURIComponent(window.location);} &apos;,
			&apos;javascript:alert(&quot;&apos; + $Label.Like_Error_Message + &apos;&quot;)&apos;
		),
		$Label.Like, &apos;_self&apos;
	) + &apos; &apos; + 
	CASE(Likes__c,
		0, &apos;&apos;,
		1, $Label.One_Like,
		SUBSTITUTE($Label.Multiple_Likes, &apos;{0}&apos;, TEXT(Likes__c))
	)
)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
